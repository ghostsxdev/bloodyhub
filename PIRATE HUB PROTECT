-- Fun√É¬ß√É¬£o para ativar automaticamente o c√É¬≥digo especificado
local function activateCode()
    local args = {
        [1] = "PickingTools",
        [2] = "Couch"
    }
    local remoteFunction = game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l")
    if remoteFunction then
        remoteFunction:InvokeServer(unpack(args))
    else
        warn("Fun√É¬ß√É¬£o remota '1Too1l' n√É¬£o encontrada em ReplicatedStorage.RE")
    end
end

